      "restBreak": {
        "description": "A rest break is a 10 minute paid break that counts as time worked.",
        "duration": 10,
        "isPaid": true,
        "countsAsTimeWorked": true
      },
      "mealBreak": {
        "description": "A meal break is a 30-60 minute unpaid break that doesn't count as time worked, except for shiftworkers.",
